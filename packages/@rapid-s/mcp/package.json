{
  "name": "@rapid-s/skills-mcp",
  "version": "0.1.0",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "x-skills-mcp": "./src/cli.ts",
    "skills-mcp": "./src/cli.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./shared": {
      "types": "./dist/shared.d.ts",
      "import": "./dist/shared.js",
      "require": "./dist/shared.cjs"
    }
  },
  "scripts": {
    "install:pkg": "bun install",
    "build:pkg": "tsup",
    "start": "bun run src/index.ts",
    "start:mcp": "bun run src/index.ts",
    "start:server": "bun run src/server/index.ts",
    "dev": "bun --watch run src/index.ts",
    "dev:server": "bun --watch run src/server/index.ts",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "codegen": "graphql-codegen --config src/server/schema/codegen.ts",
    "codegen:watch": "graphql-codegen --config src/server/schema/codegen.ts --watch",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.6",
    "@graphql-codegen/typescript-resolvers": "^5.1.4",
    "@parcel/watcher": "^2.5.1",
    "@types/bun": "^1.3.3",
    "bun-types": "^1.3.3",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "tsup": "^8.5.1",
    "typescript": "^5",
    "typescript-eslint": "^8.48.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.8.0",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@rapid-s/config": "^0.1.0",
    "@rapid-s/logger": "^0.1.0",
    "decimal.js": "^10.6.0",
    "drizzle-orm": "^0.44.7",
    "graphql": "^16.12.0",
    "graphql-yoga": "^5.17.0",
    "hono": "^4.10.7",
    "pgvector": "^0.2.1",
    "postgres": "^3.4.7",
    "reflect-metadata": "^0.2.2",
    "ts-morph": "^27.0.2",
    "tsyringe": "^4.10.0",
    "zod": "^4.1.13"
  }
}
